<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("../partials/head.ejs") %>
</head>

<body>
    <%- include("../partials/nav.ejs") %>

    <div class="container mt-5 mx-auto" style="width:30%;">
        <h2 class="text-center mb-5">Doctor Auth</h2>
          <form action="/doctor/<%= id %>" method="POST">
              <div class="form-floating m-5">
                  <input  class="form-control" id="doctorCode" name="doctorCode"></input>
                  <label for="doctorCode"><h5>Enter your Doctor Code</h5></label>
                </div>
                <div class="form-floating m-5">
                    <input  class="form-control" id="doctorPass" name="doctorPass"></input>
                    <label for="doctorPass"><h5>Enter your Password</h5></label>
                  </div>
              
              <div class="text-center">
                <button type="submit" class="btn btn-primary">Check</button>
              </div>
            </form>     
      </div>
    


</body>

</html>